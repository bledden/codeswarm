# CodeSwarm Environment Variables
# Copy this file to .env and fill in your actual API keys

# ============================================
# CORE LLM PROVIDERS
# ============================================

# OpenRouter - Multi-model gateway (REQUIRED)
# Get your key at: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_key_here

# ============================================
# SPONSOR INTEGRATIONS (All REQUIRED for full demo)
# ============================================

# Galileo Observe - Quality evaluation
# Get your key at: https://app.galileo.ai/settings/api-keys
GALILEO_API_KEY=your_galileo_key_here
GALILEO_PROJECT=codeswarm-hackathon
GALILEO_CONSOLE_URL=https://app.galileo.ai

# Neo4j Aura - RAG storage
# Create free instance at: https://neo4j.com/cloud/aura/
NEO4J_URI=neo4j+s://xxxxx.databases.neo4j.io
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password_here

# WorkOS - Team authentication
# Get your keys at: https://dashboard.workos.com/api-keys
WORKOS_API_KEY=your_workos_api_key_here
WORKOS_CLIENT_ID=your_workos_client_id_here
# AuthKit configuration (required for full OAuth flow)
WORKOS_REDIRECT_URI=http://localhost:3000/callback
# Generate with: openssl rand -base64 24
WORKOS_COOKIE_PASSWORD=your_32_character_cookie_password

# Daytona - Workspace deployment
# Get your keys at: https://app.daytona.io/settings/api-keys
DAYTONA_API_URL=https://app.daytona.io/api
DAYTONA_API_KEY=your_daytona_key_here

# Tavily - Documentation scraping
# Get your key at: https://tavily.com
TAVILY_API_KEY=your_tavily_key_here

# W&B Weave - Observability (OPTIONAL)
# Get your key at: https://wandb.ai/authorize
WANDB_API_KEY=your_wandb_key_here
WANDB_PROJECT=codeswarm-hackathon
